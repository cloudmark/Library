{
    "version": "1.0.0-*",
    "webroot": "wwwroot",
    "exclude": [
        "wwwroot",
        "bower_components",
        "node_modules",
        "grunt"
    ],
    "packExclude": [
        "bower.json",
        "package.json",
        "gruntfile.js",
        "bower_components",
        "node_modules",
        "grunt"
    ],
    "authors": [
        "Mark Galea"
    ],
