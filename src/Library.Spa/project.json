    "dependencies": {
        "Kestrel": "1.0.0-beta6-*",
        "Microsoft.AspNet.Diagnostics": "1.0.0-beta6-*",
        "Microsoft.AspNet.Diagnostics.Entity": "7.0.0-beta6-*",
        "Microsoft.AspNet.Mvc": "6.0.0-beta6-*",
        "Microsoft.AspNet.Http":  "1.0.0-beta6-*",
        "Microsoft.AspNet.Server.WebListener": "1.0.0-beta6-*",
        "Microsoft.AspNet.Authorization": "1.0.0-beta6-*",
        "Microsoft.AspNet.StaticFiles": "1.0.0-beta6-*",
        "AutoMapper": "4.0.0-*",
        "Microsoft.AspNet.Authentication.Cookies": "1.0.0-beta6-*",
        "Microsoft.Framework.Configuration": "1.0.0-beta6-*",
        "Microsoft.Framework.Configuration.Json": "1.0.0-beta6-*",
        "Newtonsoft.Json": "7.0.1",
        "Spa.Extensions": "1.0.0-beta6",
        "Library": "",
    
        "Microsoft.AspNet.Hosting": "1.0.0-beta6-*",
        "Microsoft.AspNet.Server.IIS": "1.0.0-beta6-*",
        "Microsoft.AspNet.Session": "1.0.0-beta6-*",
